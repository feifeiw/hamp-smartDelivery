/* static/css/common.wxss */
.fl{
  float: left;
}
.fr{
  float: right;
}
.IndexWrapper{
  width: 750rpx;
  min-height: 1000rpx;
  background: #f9f9f9;
  position: relative;
  box-sizing: border-box;
}
.tabcontent {
  width: 100%;
  background: #fff;
  border-top: 1px solid #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  box-sizing: border-box;
}
.tranferInfo{
  padding: 30rpx;
  width: 100%;
  background: #fff;
  box-sizing: border-box;
}
.weui-cell-list{
  background: #fff;
  padding: 0 50rpx;
}
.weui-cell-line{
  margin-left: 16rpx;
  border-left: 1px solid #f1f1f1;
  padding: 0 0 20rpx 20rpx;
  background: #fff;
}
.weui-cell-content{
  background: #f9f9f9;
  border-radius: 8rpx;
  padding: 16rpx 30rpx;
  box-sizing: border-box;
  position: relative;
}
.phone{
  position: absolute;
  right: 28rpx;
  top: 28rpx;
}
.phone image{
  width: 54rpx;
  height: 54rpx;
}
.weui-cell-circle{
  border: 6rpx solid #5275f5;
  border-radius: 50%;
  width: 20rpx;
  height: 20rpx;
  background: #fff;
}
.weui-cell-event{
  line-height: 54rpx;
  font-size: 30rpx;
  color: #999;
}
.weui-cell-name{
  font-size:34rpx;
  margin: 20rpx 0;
}
.tranferTop{
  width: 100%;
  background: #fff;
  margin-bottom: 16rpx;
  box-sizing: border-box;
  padding: 40rpx;
  border-top: 2rpx solid #f1f1f1;
  position: relative;
}
.tranferTop .caseNo{
  font-size: 32rpx;
  width: 100%;
  display: inline-block;
}
.tranferTop .caseNo .fl{
  width: 80%;
}
.tranferTop .caseInfo{
  font-size: 30rpx;
  color: #999;
  line-height: 50rpx;
  margin-top: 20rpx;
}
.tranferTop .arrow_right{
  position: absolute;
  right: 40rpx;
  top: 120rpx;
}
.tranferTop .arrow_right image{
  height: 30rpx;
  width: 30rpx;
}

/* 流转-案件信息 */
.caseInfo-li{
  width: 100%;
  display: flex;
}
.caseInfo-li label{
  width: 145rpx;
  flex: 0 0 145rpx;
}
.caseInfo-li view{
  width: auto;
  float: 0 0 1;
  text-align:left;
}
/* 持有 */
.tabList{
  padding: 115rpx 0 100rpx 0;
  display: inline-block;
  width: 100%;
}
.borderBt{
  background: #fff;
  padding: 0 26rpx;
}
.tabList .tabLi{
  background: #fff;
  width: 100%;
  box-sizing: border-box;
  border-bottom: 2rpx solid #f1f1f1;
  font-size: 30rpx;
  color: #333;
  line-height: 56rpx;
}
.tabList .tabLi .title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #000;
  font-size: 32rpx;
  display:block;
}
.tabList .tabLi .detail{
  padding: 0 20rpx 20rpx 20rpx;
  margin: 0;
  box-sizing: border-box;
  width: 100%;
  background: #f9f9f9;
  border-radius: 6rpx;
  margin-bottom: 18rpx;
}
.tabList .tabLi .detail_li{
  height: 80rpx;
  line-height: 80rpx;
  color: #999;
  display: block;
  width: 100%;
  font-size: 28rpx;
  border-bottom: 2rpx dashed #f1f1f1;
  margin: 0;
  box-sizing: border-box;
}
.tabList .tabLi .detail_li .fileNO{
  width: 76%;
  overflow: hidden;
  float: left;
  display: inline-block;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.tabList .transdate{
  width: 24%;
  overflow: hidden;
  float: left;
  text-align: right;
}
.transdate .linCirculImg{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.title .arrowIcon{
  height: 100%;
  width: 28rpx;
  display: inline-block;
  margin-left: 10rpx;
}
.title .arrowIcon image{
  height: 28rpx;
  width: 28rpx;
  vertical-align: middle;
}
.noMsg{
  text-align: center;
  margin-top: 180rpx;
  font-size: 34rpx;
  color: #999;
}
.uploadMsg{
  text-align: center;
  font-size: 30rpx;
  color: #999;
  padding: 30rpx;
}
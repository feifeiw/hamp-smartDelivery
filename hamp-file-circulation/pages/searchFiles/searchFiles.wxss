/* pages/files/files.wxss */
.tabUl{
  width: 100%;
  height: 90rpx;
  display: flex;
}
.tabcontent .tabcell {
  width: 33.333%;
  text-align: center;
  line-height: 90rpx;
  font-size: 32rpx;
  position: relative;
  font-weight: 600;
}
.tabcontent .active {
  color: #6f8cf4;
}
.dateIcon{
  width: 40rpx;
  height: 40rpx;
  vertical-align: -8rpx;
  margin-right:8rpx;
}
.searchBtn{
  float: right;
  background: #5275f5;
  color: #fff;
  border-radius: 40rpx !important;
  padding: 8rpx 20rpx !important;
  font-size: 30rpx !important;
  width: 120rpx !important;
  font-weight: normal !important;
  margin-top: 24rpx;
}
.picker_group {
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #999;
  background: #fff;
  display: inline-block;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.yearPicker{
  margin-right: 20rpx;
}
.picker_group .casePicker, .yearPicker {
  color: #999;
  height: 70rpx;
  line-height: 70rpx;
  padding: 0 14rpx;
  display: inline-block;
  border-radius: 6rpx;
  background: #f1f1f1;
  width: 36%;
  margin-top: 16rpx;
  float: left;
  box-sizing: border-box;
}
.operate{
  width: 100%;
  display: inline-block;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
}
.operate .display-block{
  display: inline-block;
  text-align: center;
}
.operate .display-block image{
  height: 40rpx;
  width: 40rpx;
  vertical-align: middle;
  margin-right: 12rpx;
}
.txtBlue{
  color: #5275f5;
}
.txtRed{
  color: #f74f4d;
}
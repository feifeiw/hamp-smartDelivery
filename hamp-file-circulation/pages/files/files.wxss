/* pages/files/files.wxss */
.IndexWrapper{
  width: 750rpx;
  min-height: 1000rpx;
  background: #f9f9f9;
  position: relative;
}
.tabcontent {
  width: 100%;
  background: #fff;
  border-top: 1px solid #f2f2f2;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.tabUl{
  width: 100%;
  height: 90rpx;
  display: flex;
}
.tabcontent .tabcell {
  width: 33.333%;
  text-align: center;
  line-height: 90rpx;
  font-size: 32rpx;
  position: relative;
  font-weight: 600;
}
.tabcontent .active {
  color: #6f8cf4;
}
.dateIcon{
  width: 40rpx;
  height: 40rpx;
  vertical-align: -8rpx;
  margin-right:8rpx;
}
.searchBtn{
  float: right;
  background: #5275f5;
  color: #fff;
  border-radius: 40rpx !important;
  padding: 8rpx 20rpx !important;
  font-size: 30rpx !important;
  width: 120rpx !important;
  font-weight: normal !important;
  margin-top: 16rpx;
}
.picker_group {
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  font-size: 30rpx;
  color: #999;
  background: #fff;
  display: inline-block;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.picker_group picker {
  color: #999;
  height: 55rpx;
  line-height: 55rpx;
  padding: 0 8rpx;
  display: inline-block;
  border-radius: 30rpx;
}
.tabList{
  padding: 200rpx 0 100rpx 0;
  display: inline-block;
  width: 100%;
}
.tabList .tabLi{
  background: #fff;
  width: 100%;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  margin-bottom: 20rpx;
  font-size: 30rpx;
  color: #333;
  line-height: 56rpx;
}
.tabList .tabLi text{
  color: #999;
}
.noMsg{
  text-align: center;
  margin-top: 180rpx;
  font-size: 34rpx;
  color: #999;
}
.uploadMsg{
  text-align: center;
  font-size: 30rpx;
  color: #999;
  padding: 30rpx;
}
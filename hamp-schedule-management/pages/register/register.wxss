/* pages/register/register.wxss */
.regiester{
  width: 750rpx;
  padding:80rpx 60rpx;
  box-sizing: border-box;
}
.tips{
  font-size: 36rpx;
  font-weight: bold;
  color: #000;
  margin-bottom: 80rpx;
}
input{
  width: 630rpx;
  background: #f5f5f5;
  font-size: 34rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin-bottom: 16rpx;
  border-radius: 6rpx;
  padding: 0 28rpx;
  color: #999;
  box-sizing: border-box;
}
.regiester_form{
  width: 630rpx;
  display: inline-block;
}
.regiester_item{
  position: relative;
}
.code{
  position: absolute;
  right: 28rpx;
  top: 0;
  font-size: 34rpx;
  color: #5275f5;
  padding: 26rpx 10rpx;
}
.regiesterBtn{
  background: #5275f5;
  color: #fff;
  font-size: 38rpx;
  width: 630rpx !important;
  height: 100rpx;
  border-radius: 50rpx;
  margin-top: 80rpx;
}
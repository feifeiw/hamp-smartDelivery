/**index.wxss**/
.IndexContainer{
  width: 750rpx;
  height: 1000rpx;
  background: #f4f4f4;
  box-sizing: border-box;
}
.service{
  position: fixed;
  right: 30rpx;
  bottom: 20rpx;
  z-index: 999;
}
.service image{
  width: 80rpx;
  height: 80rpx;
}
.datePicker{
  width: 750rpx;
  background: #fff;
  position: relative;
  z-index: 1;
}
.dateNote{
  width: 750rpx;
  height: 605rpx;
  background: #fff;
  box-shadow: 4px 1px 0 0 rgba(0,0,0,0.2);
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 9;
}
.heightAuto.dateNote{
  height: 900rpx !important;
  transition: width 2s; 
  -webkit-transition: width 2s;
}
.choseDate{
  padding: 0 40rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #eee;
  position:absolute;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  background: #fff;
}
.day{
  height: 70rpx;
  display: inline-block;
}
.arrow{
  width: 100%;
  padding:10rpx 0 0 0;
  text-align: center;
}
.arrow image{
  width: 40rpx;
  height: 40rpx;
}
.noteUl{
  min-height: 450rpx;
  height: calc(100% - 100rpx);
  position: relative;
  z-index: 9;
  padding: 18rpx 40rpx;
  width: 100%;
  box-sizing: border-box;
  font-size: 32rpx;
  overflow-y: auto;
}
.noteLi{
  background: #f4f4f4;
  border-radius: 6rpx;
  padding: 20rpx 40rpx;
  width: 100%;
  color: #999;
  line-height: 60rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
}
.noteLi_block{
  width: 100%;
  display: inline-block;
}
.heightAuto image{
  transition: width 2s; 
  -webkit-transition: width 2s;
  transform: rotate(180deg);
}